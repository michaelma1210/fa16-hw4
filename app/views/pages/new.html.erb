<%= form_tag cats_create_path do %>

<div>
    <%= label_tag(:name, "Name: ") %>
    <%= text_field_tag(:name) %>



    <%= submit_tag("Create Cat") %>


</div>
    <div>
    <%= label_tag(:tasks, "Task: ") %>
    <%= text_field_tag(:tasks) %>


    <%= label_tag(:finished, "Finished: ") %>
    <%= text_field_tag(:finished) %>


    <%= submit_tag("Create Todo") %>


</div>
    <div>

    <%= label_tag(:username, "Username: ") %>
    <%= text_field_tag(:username) %>


    <%= label_tag(:email, "Email: ") %>
    <%= text_field_tag(:email) %>


    <%= label_tag(:age, "Age: ") %>
    <%= text_field_tag(:age) %>


    <%= submit_tag("Create User") %>
    </div>
<% end %>
